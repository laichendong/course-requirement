		ReturnMessage::AddPlatformOperatorSSDCallMessage3Return(AddPlatformOperatorSSDCallMessage3ReturnReceivingEnd -> AddPlatformOperatorSSDCallMessage3ReturnReceivingEnd AddPlatformOperatorSSDCallMessage3)
		CallMessage::AddPlatformOperatorSSDCallMessage5(inputOperatorInfo AddPlatformOperatorSSDCallMessage5SendingEnd -> AddPlatformOperatorSSDExecution1StartEnd)
		ReturnMessage::AddPlatformOperatorSSDCallMessage5Return(AddPlatformOperatorSSDCallMessage3SendingEnd -> AddPlatformOperatorSSDCallMessage5ReturnReceivingEnd AddPlatformOperatorSSDCallMessage5)

		Execution::AddPlatformOperatorSSDExecution1(AddPlatformOperatorService AddPlatformOperatorSSDExecution1StartEnd AddPlatformOperatorSSDExecution1FinishEnd)

		MessageEnd::AddPlatformOperatorSSDCallMessage5SendingEnd(Administrator AddPlatformOperatorSSDCallMessage5)
		ExecutionEnd::AddPlatformOperatorSSDExecution1StartEnd(AddPlatformOperatorService AddPlatformOperatorSSDExecution1)
		ExecutionEnd::AddPlatformOperatorSSDExecution1FinishEnd(AddPlatformOperatorService AddPlatformOperatorSSDExecution1)
		MessageEnd::AddPlatformOperatorSSDCallMessage5ReturnReceivingEnd(Administrator AddPlatformOperatorSSDCallMessage5Return)
		MessageEnd::AddPlatformOperatorSSDCallMessage1SendingEnd(Administrator AddPlatformOperatorSSDCallMessage1)
		MessageEnd::AddPlatformOperatorSSDCallMessage1ReturnReceivingEnd(Administrator returnCheckResult)
		MessageEnd::AddPlatformOperatorSSDCallMessage3SendingEnd(Administrator AddPlatformOperatorSSDCallMessage3)
		MessageEnd::AddPlatformOperatorSSDCallMessage3ReturnReceivingEnd(Administrator AddPlatformOperatorSSDCallMessage3Return)

	}

	Service QMSSystem {

	}

	Service ThirdPartyServices {

	}

	Service Usecase1Service {

	}

	Service PublishTaskService {

	}

	Service AddSupplierService {

		[Operation]
		addSupplier()

	}

	Service AddPlatformOperatorService {

		[Operation]
		saveOperator(operator)
		saveOperatorInfo()
		inputOperatorInfo()

	}

	Contract AddPlatformOperatorService::saveOperator(operator :Operator) : Boolean {

		/* precondition: skeleton */
		precondition:
			true

