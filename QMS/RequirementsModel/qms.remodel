DomainModel QMS {

	Entity Supplier extends User {
		@Description( "" ) Type : SupplierType[QC|DIV]
	}

	Entity Task {
		Id : String
		Name : String
		Content : String
		Publisher : String
		Price : String
		Excutor : String

		[Refer]
		TasktoSupplier : Supplier Association
		TasktoOperator : Operator Association
	}

	Entity Operator extends User {
		@Description( "" ) }

	Entity User {
		@Description( "" ) User_id : String
		User_name : String
	}

}

