		MessageEnd::AddSuplierSSDCallMessage1SendingEnd(PlatformOperator AddSuplierSSDCallMessage1)
		ExecutionEnd::AddSuplierSSDExecution1StartEnd(AddSupplierService AddSuplierSSDExecution1)
		ExecutionEnd::AddSuplierSSDExecution1FinishEnd(AddSupplierService AddSuplierSSDExecution1)
		MessageEnd::AddSuplierSSDCallMessage1ReturnReceivingEnd(PlatformOperator AddSuplierSSDCallMessage1Return)

	}

	Interaction AddPlatformOperatorSSD {

