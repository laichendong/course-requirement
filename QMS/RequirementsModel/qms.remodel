UseCaseModel QMS {

	UC::addSupplier definedBySSD(AddSupplierSSD) relatedService(AddSupplierService)
	UC::publishTask definedBySSD(PublishTaskSSD) relatedService(PublishTaskService)
	UC::recieveReport
	UC::settleAccount
	UC::recieveTask
	UC::makeReport
	UC::reconcile
	UC::addPlatformOperator include inputPlatformOperatorInfo, savePlatformOperatorInfo definedBySSD(AddPlatformOperatorSSD) relatedService(AddPlatformOperatorService)
	UC::inputPlatformOperatorInfo
	UC::savePlatformOperatorInfo

	Actor Administrator {
		addPlatformOperator
	}

	Actor Supplier {
		recieveTask
		makeReport
		reconcile
	}

	Actor PlatformOperator {
		addSupplier
		publishTask
		recieveReport
		settleAccount
	}

	Interaction PublishTaskSSD {

		[Participants: PlatformOperator PublishTaskService]

	}

	Interaction AddSupplierSSD {

